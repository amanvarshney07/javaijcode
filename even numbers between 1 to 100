import java .util.*;
public class even
{
    public static void main(String[]args){
        int n;
        Scanner Sc = new Scanner(System.in);
        n = Sc.nextInt();
        for(int i=1;i<n+1;i++){
           if(i%2==0){
               System.out.println(i);
           }
    }
        
    }
    
}

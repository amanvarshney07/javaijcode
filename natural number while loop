import java.util.*;
public class naturalnumber {
  public static void main(String[] args) {
    int n,i;
    Scanner Sc = new Scanner(System.in);
    System.out.println("enter the value of n :");
    n = Sc.nextInt();
    i = 1;
    while(i<=n){
        System.out.println(i);
        i++;
    }
  }  
}

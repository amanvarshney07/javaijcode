import java.util.Scanner;
public class positive{
    public static void main(String[]args){
        Scanner Sc = new Scanner(System.in);
        int x = Sc.nextInt();
        int y = Sc.nextInt();
        int q = Sc.nextInt();
        switch(q>0?1:0){
            case 0 :
                System.out.println("negative");
                break;
            case 1:
                System.out.println("positive");
                break;
            default:
                System.out.println("zero");
        }
    }
}
